{
    "venvPath": ".",
    "venv": ".venv",
    "pythonVersion": "3.13",
    "pythonPlatform": "Linux",
    "extraPaths": [
        "./.venv/lib/python3.13/site-packages"
    ],
    "typeCheckingMode": "basic",
    "reportMissingImports": "warning",
    "reportMissingTypeStubs": false,
    "include": [
        "."
    ],
    "exclude": [
        "**/__pycache__",
        ".venv"
    ]
}